issn = {2050-084X},
journal = {eLife},
keywords = {Alleles,Animals,Biostatistics,Cerebellum,Cerebellum: physiology,DNA,Gene Expression Profiling,Genomic Imprinting,Inbred C57BL,Mice,Molecular Sequence Data,Sequence Analysis,expression,genomics,imprinting},
language = {en},
